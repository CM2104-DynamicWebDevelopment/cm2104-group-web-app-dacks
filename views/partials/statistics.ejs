<!-- views/partials/statistics.ejs -->
<!-- Modal to store the statistics for covid-19 -->
<div class="modal fade" id="statisticsModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <!-- Modal header with heading -->
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel"><%- statistics %></h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      
      <!-- Modal with blank body (info will be fed dynamically) -->
      <div class="modal-body">
      </div>
      
      <!-- Modal footer with buttons -->
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal"><%- close %></button>
      </div>
    </div>
  </div>
</div>
