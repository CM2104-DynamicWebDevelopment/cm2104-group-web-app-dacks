<div class="modal fade" id="contactModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel"><%- sendEmail.heading %></h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <form>
          <div class="form-group">
            <label for="user-name" class="col-form-label"><%- sendEmail.name %></label>
            <input type="text" class="form-control" id="user-name">
          </div>
          <div class="form-group">
            <label for="Email" class="col-form-label"><%- sendEmail.email %></label>
            <input type="text" class="form-control" id="Email">
          </div>
          <div class="form-group">
            <label for="message-text" class="col-form-label"><%- sendEmail.message %></label>
            <textarea class="form-control" id="message-text"></textarea>
          </div>
        </form>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal"><%- close %></button>
        <button type="button" class="btn btn-primary"><%- sendEmail.send %></button>
      </div>
    </div>
  </div>
</div>
